    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "friendsofphp/php-cs-fixer": "^3.62",
        "phpstan/phpstan": "^1.11"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
